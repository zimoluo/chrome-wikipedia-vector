{
    "manifest_version": 2,
    "name": "Wikipedia Vector Skin",
    "description": "Add 'useskin=vector' to Wikipedia URLs",
    "version": "0.1",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["./scripts/background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://*.wikipedia.org/*"],
            "js": ["./scripts/content.js"]
        }
    ]
}
